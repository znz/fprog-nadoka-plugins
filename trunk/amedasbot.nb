begin
  require 'rubygems'
rescue LoadError
end
require 'nokogiri'

require 'nkf'
require 'open-uri'
require 'timeout'

module AMeDAS
  class AreaError < RuntimeError; end
  class DuplicatedArea < AreaError; end
  class UnknownArea < AreaError; end

  module_function

  # load data generated by amedas_h.rb
  begin
    orig_kcode, $KCODE = $KCODE, 'u' unless "".respond_to?(:encode)
    src = NKF.nkf('-Ww', File.read(__FILE__)).split(/^__END__$/)
    eval src[1], binding, __FILE__, src[0].count("\n")+1
  ensure
    $KCODE = orig_kcode unless "".respond_to?(:encode)
  end

  INVERT_GROUP_CODES = GROUP_CODES.invert

  def area_to_uri(area, day='today')
    area = NKF.nkf("-w", area)
    area, group = area.split(/\s+/, 2)
    g_code = nil
    if group
      g_code = INVERT_GROUP_CODES[group]
      raise UnknownArea, "unknown group: #{group}" unless g_code
    end
    if !g_code && DUPLICATED_AREA.include?(area)
      msg = "duplicated area: #{area}: "
      msg << AREAS.select do |name, code, group_code|
        area == name
      end.map do |name, code, group_code|
        "#{GROUP_CODES[group_code]}(#{code})"
      end.join(", ")
      raise DuplicatedArea, msg
    end

    AREAS.each do |name, code, group_code|
      if area == name || area == code
        next if g_code && g_code != group_code
        return URI.parse("http://www.jma.go.jp/jp/amedas_h/#{day}-#{code}.html?groupCode=#{group_code}&areaCode=000")
      end
    end

    raise UnknownArea, "unknown area #{area}"
  end

  def get(area, timeout=10)
    uri = area_to_uri(area)
    doc = Timeout.timeout(timeout) do
      uri.open{|f| Nokogiri::HTML(NKF.nkf("-w", f.read), nil, "utf-8") }
    end

    td = doc.xpath('id("tbl_list")/tr[contains(td[2],".")][position()=last()]/td').map{|x|x.text}
    if td.empty?
      # retry when 24:mm
      uri = area_to_uri(area, 'yesterday')
      doc = Timeout.timeout(timeout) do
        uri.open{|f| Nokogiri::HTML(NKF.nkf("-w", f.read), nil, "utf-8") }
      end
      td = doc.xpath('id("tbl_list")/tr[contains(td[2],".")][position()=last()]/td').map{|x|x.text}
    end
    title = doc.css(".td_title").text
    th1 = doc.xpath('id("tbl_list")/tr[1]/td').map{|x|x.text}
    th2 = doc.xpath('id("tbl_list")/tr[2]/td').map{|x|x.text}
    ret = []
    th1.zip(td, th2) do |h, d, unit|
      if /\d/ =~ d
        ret << "#{h}:#{d}#{unit}"
      else
        ret << "#{h}:#{d}"
      end
    end
    latest = "#{title}  " + ret.join(", ") + "  #{uri}"

    table = doc.xpath('id("tbl_temperature")/tr[position()!=1]/td').map{|x|x.text}
    ret = []
    table.each_slice(3) do |name, data, time|
      ret << "#{name}:#{data} at #{time}"
    end
    temperature = "#{title}  " + ret.join(", ")

    [latest, temperature]
  end
end

if __FILE__ == $0
  puts AMeDAS.get('大阪')
  ARGV.each do |area|
    puts AMeDAS.get(area)
  end
  exit
end

class AmedasBot < Nadoka::NDK_Bot
  include AMeDAS

  def bot_initialize
    if @bot_config.key?(:channels)
      channels = '\A(?:' + @bot_config[:channels].collect{|ch|
        Regexp.quote(ch)
      }.join('|') + ')\z'
      @available_channel = Regexp.compile(channels)
    else
      @available_channel = @bot_config.fetch(:ch, //)
    end

    @bot_name = @bot_config[:bot_name] || 'amedas'
    @separator = @bot_config[:separator] || /[<:>]/
    @pattern = /^#{Regexp.quote(@bot_name)}\s*#{@separator}\s*(.+)/

    @same_bot = @bot_config.fetch(:same_bot, /(?!)/)
    @nkf_options = @bot_config.fetch(:nkf, "-j")
    @timeout = @bot_config.fetch(:timeout, 10)
  end

  def send_notice(ch, msg)
    msg = msg.tr("\r\n", "  ")
    if @nkf_options
      msg = NKF.nkf(@nkf_options, msg)
    end
    super(ch, msg)
  end

  def on_privmsg prefix, ch, msg
    return unless @available_channel === ch

    if @pattern === msg
      area = $1.strip
      return if @state.channel_users(ccn(ch)).find{|x| @same_bot =~ x }
      latest, temperature = AMeDAS.get(area, @timeout)
      send_notice(ch, "#{@bot_name}: #{latest}")
      send_notice(ch, "#{@bot_name}: #{temperature}")
    end
  rescue AreaError => e
    send_notice(ch, "#{@bot_name}: #{e}")
  rescue Exception => e
    send_notice(ch, "#{@bot_name}: #{e}, see http://www.jma.go.jp/jp/amedas_h/index.html instead")
    @manager.ndk_error e
  end
end

__END__
